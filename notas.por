programa {
  funcao inicio() {
    inteiro N1, N2, N3, N4
    escreva("Digite a primeira nota:")
    leia(N1)
    escreva("Digite a segunda nota:")
    leia(N2)
    escreva("Digite a terceira nota:")
    leia(N3)
    escreva("Digite a quarta nota:")
    leia(N4)
    inteiro media = (N1+ N2+ N3+ N4) / 3
    escreva("A média das notas é " + media)
    
  }
}
