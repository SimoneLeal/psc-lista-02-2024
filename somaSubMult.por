programa {
  funcao inicio() {
    inteiro N1, N2, soma, sub, mult
    escreva("Digite um número:")
    leia (N1)
    escreva("Digite um segundo número:")
    leia (N2)
    soma=N1+N2
    sub=N1-N2
    mult=N1*N2
    escreva ("A soma dos números é " ,soma, "\n",
     "A subtração dos número é " ,sub, "\n", 
    "A multplicação dos números é " ,mult)
    

    
  }
}
